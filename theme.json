		"custom": {
			"border": {
				"color": "var(--wp--preset--color--neutral-200)",
				"radius": "8px"
			},
			"boxShadow": {
				"inset": "",
				"x": "0",
				"y": "4px",
				"blur": "20px",
				"spread": "0",
				"color": "var(--wp--preset--color--custom-shadow,rgba(0,20,40,0.04))",
				"hover": {
					"inset": "",
					"y": "8px"
				}
			},
			"transition": "all 0.2s ease",
			"placeholder": {
				"background": "var(--wp--preset--color--neutral-100)"
			},
			"icon": {
				"color": "var(--wp--preset--color--primary-500)"
			},
			"checklist": {
				"background": "var(--wp--preset--gradient--primary-500)"
			},
			"submenu": {
				"borderRadius": "8px"
			},
			"input": {
